<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
		"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
		"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
<class name="com.wkmk.tk.bo.TkBookFileDownload" table="tk_book_file_download">
<id name="downloadid" type="java.lang.Integer" column="downloadid">  <generator class="native" /> </id>
<property  name="fileid" type="java.lang.Integer" column="fileid"/>
<property  name="userid" type="java.lang.Integer" column="userid"/>
