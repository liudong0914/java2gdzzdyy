<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
		"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
		"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
<class name="com.wkmk.tk.bo.TkClassPassword" table="tk_class_password">
<id name="passwordid" type="java.lang.Integer" column="passwordid">  <generator class="native" /> </id>
<property  name="password" type="java.lang.String" column="password"/>
<property  name="used" type="java.lang.String" column="used"/>
<property  name="userid" type="java.lang.Integer" column="userid"/>
<property  name="classid" type="java.lang.Integer" column="classid"/>
<property  name="unitid" type="java.lang.Integer" column="unitid"/>
</class>
</hibernate-mapping>