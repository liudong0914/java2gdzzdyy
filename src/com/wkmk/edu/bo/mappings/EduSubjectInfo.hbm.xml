<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
		"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
		"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
<class name="com.wkmk.edu.bo.EduSubjectInfo" table="edu_subject_info">
<id name="subjectid" type="java.lang.Integer" column="subjectid">  <generator class="native" /> </id>
<property  name="subjectname" type="java.lang.String" column="subjectname"/>
<property  name="fullname" type="java.lang.String" column="fullname"/>
<property  name="banner" type="java.lang.String" column="banner"/>
<property  name="orderindex" type="java.lang.Integer" column="orderindex"/>
<property  name="status" type="java.lang.String" column="status"/>
<property  name="updatetime" type="java.lang.String" column="updatetime"/>
</class>
</hibernate-mapping>