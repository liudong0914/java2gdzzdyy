<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
		"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
		"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
<class name="com.wkmk.sys.bo.SysUnitDept" table="sys_unit_dept">
<id name="deptid" type="java.lang.Integer" column="deptid">  <generator class="native" /> </id>
<property  name="deptname" type="java.lang.String" column="deptname"/>
<property  name="deptno" type="java.lang.String" column="deptno"/>
<property  name="parentno" type="java.lang.String" column="parentno"/>
<property  name="telephone" type="java.lang.String" column="telephone"/>
<property  name="fax" type="java.lang.String" column="fax"/>
<property  name="leader" type="java.lang.String" column="leader"/>
<property  name="status" type="java.lang.String" column="status"/>
<property  name="unitid" type="java.lang.Integer" column="unitid"/>
</class>
</hibernate-mapping>
