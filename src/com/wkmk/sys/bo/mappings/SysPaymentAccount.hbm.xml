<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
		"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
		"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
<class name="com.wkmk.sys.bo.SysPaymentAccount" table="sys_payment_account">
<id name="paymentid" type="java.lang.Integer" column="paymentid">  <generator class="native" /> </id>
<property  name="fromuserid" type="java.lang.Integer" column="fromuserid"/>
<property  name="fromuserip" type="java.lang.String" column="fromuserip"/>
<property  name="touserid" type="java.lang.Integer" column="touserid"/>
<property  name="changemoney" type="java.lang.Float" column="changemoney"/>
<property  name="changetype" type="java.lang.Integer" column="changetype"/>
<property  name="outtype" type="java.lang.String" column="outtype"/>
<property  name="outobjid" type="java.lang.Integer" column="outobjid"/>
<property  name="createdate" type="java.lang.String" column="createdate"/>
<property  name="happendate" type="java.lang.String" column="happendate"/>
<property  name="status" type="java.lang.String" column="status"/>
</class>
</hibernate-mapping>