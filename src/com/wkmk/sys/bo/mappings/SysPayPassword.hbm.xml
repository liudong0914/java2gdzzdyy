<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
		"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
		"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
<class name="com.wkmk.sys.bo.SysPayPassword" table="sys_pay_password">
<id name="paypasswordid" type="java.lang.Integer" column="paypasswordid">  <generator class="native" /> </id>
<property  name="userid" type="java.lang.Integer" column="userid"/>
<property  name="userip" type="java.lang.String" column="userip"/>
<property  name="createdate" type="java.lang.String" column="createdate"/>
<property  name="password" type="java.lang.String" column="password"/>
</class>
</hibernate-mapping>