<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
		"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
		"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
<class name="com.wkmk.zx.bo.ZxHomeworkFile" table="zx_homework_file">
<id name="fileid" type="java.lang.Integer" column="fileid">  <generator class="native" /> </id>
<property  name="filepath" type="java.lang.String" column="filepath"/>
<property  name="mp3path" type="java.lang.String" column="mp3path"/>
<property  name="thumbpath" type="java.lang.String" column="thumbpath"/>
<property  name="filesize" type="java.lang.Long" column="filesize"/>
<property  name="filetype" type="java.lang.String" column="filetype"/>
<property  name="convertstatus" type="java.lang.String" column="convertstatus"/>
<property  name="timelength" type="java.lang.Integer" column="timelength"/>
<property  name="createdate" type="java.lang.String" column="createdate"/>
<property  name="type" type="java.lang.String" column="type"/>
<property  name="questionid" type="java.lang.Integer" column="questionid"/>
<property  name="answerid" type="java.lang.Integer" column="answerid"/>
</class>
</hibernate-mapping>