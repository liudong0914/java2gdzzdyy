<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
		"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
		"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
<class name="com.wkmk.cms.bo.CmsImageInfo" table="cms_image_info">
<id name="imageid" type="java.lang.Integer" column="imageid">  <generator class="native" /> </id>
<property  name="title" type="java.lang.String" column="title"/>
<property  name="url" type="java.lang.String" column="url"/>
<property  name="createdate" type="java.lang.String" column="createdate"/>
<property  name="sketch" type="java.lang.String" column="sketch"/>
<property  name="status" type="java.lang.String" column="status"/>
<property  name="orderindex" type="java.lang.Integer" column="orderindex"/>
<property  name="unitid" type="java.lang.Integer" column="unitid"/>
</class>
</hibernate-mapping>