<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
		"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
		"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
<class name="com.wkmk.edu.bo.EduCourseFile" table="edu_course_file">
<id name="fileid" type="java.lang.Integer" column="fileid">  <generator class="native" /> </id>
<property  name="filename" type="java.lang.String" column="filename"/>
<property  name="filepath" type="java.lang.String" column="filepath"/>
<property  name="pdfpath" type="java.lang.String" column="pdfpath"/>
<property  name="pagenum" type="java.lang.Integer" column="pagenum"/>
<property  name="sketch" type="java.lang.String" column="sketch"/>
<property  name="filesize" type="java.lang.Long" column="filesize"/>
<property  name="fileext" type="java.lang.String" column="fileext"/>
<property  name="createdate" type="java.lang.String" column="createdate"/>
<property  name="notifystatus" type="java.lang.String" column="notifystatus"/>
<property  name="convertstatus" type="java.lang.String" column="convertstatus"/>
<property  name="filetype" type="java.lang.String" column="filetype"/>
<property  name="coursefiletype" type="java.lang.String" column="coursefiletype"/>
<property  name="hits" type="java.lang.Integer" column="hits"/>
<property  name="downloads" type="java.lang.Integer" column="downloads"/>
<property  name="coursecolumnid" type="java.lang.Integer" column="coursecolumnid"/>
<property  name="courseid" type="java.lang.Integer" column="courseid"/>
<property  name="userid" type="java.lang.Integer" column="userid"/>
</class>
</hibernate-mapping>