<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
		"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
		"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
<class name="com.wkmk.edu.bo.EduBookInfo" table="edu_book_info">
<id name="bookid" type="java.lang.Integer" column="bookid">  <generator class="native" /> </id>
<property  name="title" type="java.lang.String" column="title"/>
<property  name="subtitle" type="java.lang.String" column="subtitle"/>
<property  name="orderindex" type="java.lang.Integer" column="orderindex"/>
<property  name="status" type="java.lang.String" column="status"/>
<property  name="price" type="java.lang.Float" column="price"/>
<property  name="sketch" type="java.lang.String" column="sketch"/>
<property  name="coursetypeid" type="java.lang.String" column="coursetypeid"/>
<property  name="createdate" type="java.lang.String" column="createdate"/>
</class>
</hibernate-mapping>