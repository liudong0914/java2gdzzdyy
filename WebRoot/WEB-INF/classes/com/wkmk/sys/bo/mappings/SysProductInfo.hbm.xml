<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
		"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
		"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
<class name="com.wkmk.sys.bo.SysProductInfo" table="sys_product_info">
<id name="productid" type="java.lang.Integer" column="productid">  <generator class="native" /> </id>
<property  name="productno" type="java.lang.String" column="productno"/>
<property  name="productname" type="java.lang.String" column="productname"/>
<property  name="shortname" type="java.lang.String" column="shortname"/>
<property  name="status" type="java.lang.String" column="status"/>
<property  name="type" type="java.lang.String" column="type"/>
<property  name="sketch" type="java.lang.String" column="sketch"/>
<property  name="linkurl" type="java.lang.String" column="linkurl"/>
<property  name="orderindex" type="java.lang.Integer" column="orderindex"/>
<property  name="descript" type="java.lang.String" column="descript"/>
</class>
</hibernate-mapping>
