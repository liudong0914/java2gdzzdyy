<property  name="createdate" type="java.lang.String" column="createdate"/>
<property  name="twocodepath" type="java.lang.String" column="twocodepath"/>
<property  name="bookid" type="java.lang.Integer" column="bookid"/>
<property  name="userid" type="java.lang.Integer" column="userid"/>
<property  name="unitid" type="java.lang.Integer" column="unitid"/>
<property  name="uploadimg" type="java.lang.String" column="uploadimg"/>
</class>
</hibernate-mapping>